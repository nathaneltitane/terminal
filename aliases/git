# git #

[[ -x $(command -v git) ]] && alias 'gc'='git clone'
[[ -x $(command -v git) ]] && alias 'gi'='git init'
[[ -x $(command -v git) ]] && alias 'gp'='git pull'
[[ -x $(command -v git) ]] && alias 'ga'='git add *'
[[ -x $(command -v git) ]] && alias 'gm'="git commit -a -m ${1}"
[[ -x $(command -v git) ]] && alias 'gs'='git push'
[[ -x $(command -v git) ]] && alias 'gu'='ga && gm "Code & Features Update" && gs'