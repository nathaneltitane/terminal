# system #

[[ $(command -v systemctl) ]] && alias 'start'='systemctl start'
[[ $(command -v systemctl) ]] && alias 'stop'='systemctl stop'
[[ $(command -v systemctl) ]] && alias 'enable'='systemctl enable'
[[ $(command -v systemctl) ]] && alias 'disable'='systemctl disable'
[[ $(command -v systemctl) ]] && alias 'status'='systemctl status'