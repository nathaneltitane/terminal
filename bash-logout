#!/bin/bash

# dextop ///////////////////////////////////////////////////////////////////////

if [ -f "${HOME}"/.dextop/dextop ]
then
	# automatic vnc session stop

	if [[ $(cat "${HOME}"/.dextop/dextop-logout) = "logout" ]]
	then
		if [[ $(pidof Xvnc) ]]
		then
			container-session -x
		fi
	fi
fi
